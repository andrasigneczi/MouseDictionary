<workbook>
  <worksheet name="Sheet1">
    <section startRow="0" endRow="0" />
    <loop startRow="1" endRow="1" items="result" var="translation" varType="recognizer.Translation">
      <section startRow="1" endRow="1">
        <mapping row="1" col="0">translation.from</mapping>
        <mapping row="1" col="1">translation.to</mapping>
        <mapping row="1" col="2">translation.key</mapping>
        <mapping row="1" col="3">translation.value</mapping>
      </section>
      <loopbreakcondition>
        <rowcheck offset="0">
          <cellcheck offset="0" />
        </rowcheck>
      </loopbreakcondition>
    </loop>
  </worksheet>
</workbook>